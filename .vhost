# порт, к которому привязываются <VirtualHost>'ы Apache
# по-умолчанию: (нет значения)
# наследуется
#Port 80

# порт, к которому привязываются server{}'ы nginx
# по-умолчанию: (нет значения)
# наследуется
#NginxPort 80

# количество поддоменов в глубину (осталось пройти). уменьшается на 1 при входе во вложенную директорию.
# достижение нуля означает, что данная директория есть VirtualHost (больше вглубь ходить не нужно)
# по умолчанию: (оставшееся значение из родительской директории)
#Depth 2

# Если указано, то данная директория будет проигнорирована при создании хостов.
# по-умолчанию: (отсутствует)
# наследуется
#Skip

# Использовать другое имя домена вместо имени текущей папки. Если пустая строка, то используется имя папки.
# по-умолчанию: (пусто)
# локально
#Name

# Алиасы самой папки в дополнении к ее имени.
# по-умолчанию: (пусто)
# локально
#DirAlias

# Алиасы всего домена в дополнении ко всем алиасам самих папок.
# по-умолчанию: (пусто)
# наследуется
# шаблон
#DomainAlias

# Список дополнительных директив внутрь <VirtualHost>.
# наследуется
# шаблон
Headers <<<<
>>>>

# Заменять дополнительные команды, а не дополнять
# по умолчанию: (отсутствует)
# локально
#HeadersReplace

# Список дополнительных команд внутрь server{}.
# наследуется
# шаблон
NginxHeaders <<<<
>>>>

# Заменять дополнительные команды, а не дополнять
# по умолчанию: (отсутствует)
# локально
#NginxHeadersReplace
